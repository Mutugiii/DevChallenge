{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": [],
  "rules": {
    "no-undef": "warn",
    "no-unused-vars": "off",
    "no-console": "off",
    "func-names": "off",
    "prefer-template": "off"
  }
}